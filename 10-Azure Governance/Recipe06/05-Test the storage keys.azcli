# Retrieve the keys of the storage and show the values
az cognitiveservices account keys list --name $cognitive_service_name --resource-group Recipe10-06-rg -o tsv

# Show the value of the connections string used in the App Service
az webapp config appsettings list --name $appservice_name --resource-group Recipe10-06-rg --query "[?name=='CognitiveKey'].value" -o tsv